<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
    "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
    
<mapper namespace="com.great.mapper.AisTranSactMapper">
	<select id="queryAllTran" resultType="com.great.bean.TranSact">
		select TRAN_ID "tranId",PACK_ID "packId",CAR_ID "carId",PARK_ID "parkId",to_char(TRAN_ETIME,'YYYY-MM-DD') "tranEtime",
		to_char(TRAN_STIME,'YYYY-MM-DD') "tranStime" ,TRAN_STATE "tranState" from T_TRANSACT
	</select>
	<update id="updateTranSactState" parameterType="int">
		update T_TRANSACT set TRAN_STATE=2 where TRAN_ID=#{_parameter}
	</update>
</mapper>